    "commitlint": "^19.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globby": "^14.0.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "snyk": "^1.1285.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.17.0"
  }
